{
    "env":{
        "browser": true,
        "node":true,
        "es6": true
    },
    "globals": {},
    "parser": "@typescript-eslint/parser",
    "plugins": ["typescript"],
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "no-undef": "off",
        "no-unused-vars":"off",
        "no-console":"off",
        "prettier/prettier": [
            "error",
            {
              "trailingComma": "es5"
            }
        ]
    },
    "parserOptions": {
        "sourceType": "module"
    }
}